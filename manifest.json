{
    "manifest_version": 3,
    "name": "Ansgar's Google Search Shortcuts",
    "version": "1.0",
    "description": "Adds keyboard shortcuts to Google search results.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.google.com/search*",
                "*://*.google.com/webhp*",
                "*://*.google.com/imghp*"
            ],
            "js": ["speedykeys.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "commands": {}
}
